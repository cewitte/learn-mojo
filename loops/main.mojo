fn main():
    # print numbers from 0 to 10 with for loop
    print('---')
    print('for loop:')
    for i in range(11):
        print(i)

    # print numbers from 0 to 10 with while loop
    print('---')
    var x: Int8 = 0
    print('while loop:')
    while x < 11:
        print(x)
        x += 1

